  <SQLQuery><![CDATA[select cld_id,             
sloc_id,             
org_id,             
ho_org_id,             
doc_id,             
so_id,     
2341 as BVRType     
from             
sls.sls$so         
where            
cld_id = :bindCldId and            
sloc_id = :bindSlocId and            
org_id = :bindOrgId and            
ho_org_id = :bindHoOrgId and            
-- DOC_ID = nvl(:bindDocId,doc_id)           
DOC_ID in (           
select DISTINCT(DOC_ID) from sls.sls$so$dlv$schdl           
where CLD_ID = :bindCldId           
AND SLOC_ID = :bindSlocId           
AND ORG_ID = :bindOrgId           
AND HO_ORG_ID = :bindHoOrgId           
AND DOC_ID = nvl(:bindDocId,doc_id)             
)           
UNION ALL      
select A.cld_id,             
A.sloc_id,             
A.org_id,       
'01',      
A.doc_id,             
A.TRF_NO,     
5237 as BVRType    
from             
MM.MM$TRF$ORD$BULK  A      
where            
A.cld_id = :bindCldId and            
A.sloc_id = :bindSlocId and            
A.org_id = :bindOrgId and            
EXISTS (SELECT 1 FROM MM.MM$TRF$ORD WHERE DOC_ID_SRC=A.DOC_ID AND CLD_ID=A.CLD_ID AND SLOC_ID=A.SLOC_ID AND ORG_ID=A.ORG_ID) 
/** 
* Added by : Manjeet Singh 
* Modified on : 08-12-2022 
* Remarks : This query is modified for bring BTO id '. 
* TMS ID : 132314 
**/]]></SQLQuery>
