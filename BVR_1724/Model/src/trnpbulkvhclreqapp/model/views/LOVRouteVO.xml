  <SQLQuery><![CDATA[SELECT distinct                         
A.CLD_ID,                                  
A.SLOC_ID,                                  
A.ORG_ID,                                   
A.HO_ORG_ID,                         
A.DOC_ID,                           
A.ROUTE_ID,                             
A.ROUTE_NM,                                              
A.ARRIVAL_PNT ,      
(SELECT B.ADDS_LOC FROM SLS.SLS$TRNP$ADDS$LOC_VW B  WHERE  A.CLD_ID = B.CLD_ID                         
AND A.SLOC_ID = B.SLOC_ID  AND A.ARRIVAL_PNT = B.LOC_ID  ) ARRIVAL_NM                        
FROM sls.SLS$TRNP$ROUTE_VW A  where                         
A.CLD_ID =:cldIdbind                              
AND A.SLOC_ID = :slocIdBind                              
AND A.ORG_ID = :orgIdBind                             
and A.ho_org_id = :HoOrgIdBind                        
AND A.DEPARTURE_PNT in nvl(:deptPointBind,A.DEPARTURE_PNT)                         
AND A.ARRIVAL_PNT in nvl(:arrPointBind,A.ARRIVAL_PNT)           
and A.doc_id = nvl(:RouteDocIdBind,A.doc_id)]]></SQLQuery>
