          Value="Hide"/>
      </SchemaBasedProperties>
    </Properties>
  </Variable>
  <Variable
    Name="BindAddsId"
    Kind="where"
    Type="java.lang.String">
    <Properties>
      <SchemaBasedProperties>
        <DISPLAYHINT
          Value="Hide"/>
      </SchemaBasedProperties>
    </Properties>
  </Variable>
  <Variable
    Name="BindLocId"
    Kind="where"
    Type="java.lang.String">
    <Properties>
      <SchemaBasedProperties>
        <DISPLAYHINT
          Value="Hide"/>
      </SchemaBasedProperties>
    </Properties>
  </Variable>
  <SQLQuery><![CDATA[SELECT                                         
    X.CLD_ID CLD_ID,                            
    X.SLOC_ID SLOC_ID,      
    X.ADDS_ID ADDS_ID,      
    Y.ADDS_LOC ADDS_LOC,                       
    X.LOC_ID LOC_ID                                    
FROM APP.APP$ADDS$BK$DTL X, sls.SLS$TRNP$ADDS$LOC_VW Y          
WHERE           
X.CLD_ID = :BindCldId          
AND X.SLOC_ID = :BindSlocId          
AND X.ADDS_ID = NVL(:BindAddsId , X.ADDS_ID)         
AND X.SLOC_ID = Y.SLOC_ID          
AND X.CLD_ID = Y.CLD_ID          
AND X.STRUCT_ID = Y.STRUCT_ID          
AND X.LBL_ID = Y.LBL_ID          
AND X.LOC_ID = Y.LOC_ID          
and Y.ADDS_LOC = NVL(:BindAddsLoc, Y.ADDS_LOC)  
and X.LOC_ID = NVL(:BindLocId,X.LOC_ID)]]></SQLQuery>
  <ViewAttribute
    Name="CldId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="255"
    Type="java.lang.String"
