    Kind="where"
    Type="java.lang.String">
    <Properties>
      <SchemaBasedProperties>
        <DISPLAYHINT
          Value="Hide"/>
      </SchemaBasedProperties>
    </Properties>
  </Variable>
  <Variable
    Name="attIdBind"
    Kind="where"
    Type="java.lang.Integer">
    <Properties>
      <SchemaBasedProperties>
        <DISPLAYHINT
          Value="Hide"/>
      </SchemaBasedProperties>
    </Properties>
  </Variable>
  <SQLQuery><![CDATA[SELECT      
CAT_ID ATT_ID,     
CAT_NM ATT_NM     
FROM APP.APP$CAT     
WHERE     
CAT_CLD_ID =:cldIdBind     
AND CAT_SLOC_ID = :slocIdBind      
and cat_ho_org_id = :hoOrgIdBind     
and cat_id = NVL(:attIdBind,cat_id)  
