<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="LOVVhclTypeVO"
  Version="12.1.3.10.8"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="trnpbulkvhclreqapp.model.views.LOVVhclTypeVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="cldIdBind"
    Kind="where"
    Type="java.lang.String">
