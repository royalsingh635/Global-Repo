<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="LovModeVO"
  Version="12.1.3.10.8"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="trnpbulkvhclreqapp.model.views.LovModeVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="BindLangId"
    Kind="where"
    Type="java.lang.Integer">
    <Properties>
      <SchemaBasedProperties>
        <DISPLAYHINT
          Value="Hide"/>
      </SchemaBasedProperties>
    </Properties>
  </Variable>
  <SQLQuery><![CDATA[select a.ATT_ID,      
nvl(b.ATT_NM,a.ATT_NM) ATT_NM     
from app.app$ds$att a ,          
app.app$ds$att$lang b          
where a.ATT_TYPE_ID =612   
and a.att_id <> 2466  
and a.att_id = b.att_id(+)          
and a.ATT_TYPE_ID= B.ATT_TYPE_ID(+)          
and :BindLangId = B.ATT_LANG_ID(+)         
order by ATT_NM]]></SQLQuery>
  <ViewAttribute
    Name="AttId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.lang.Integer"
    ColumnType="INTEGER"
    AliasName="ATT_ID"
    Expression="ATT_ID"
    SQLType="INTEGER"/>
  <ViewAttribute
    Name="AttNm"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="100"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="ATT_NM"
    Expression="ATT_NM"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="100"/>
    </DesignTime>
  </ViewAttribute>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="trnpbulkvhclreqapp.model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
